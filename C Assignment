6) Take two numbers from user and compare values and print results as false or true.

#include<stdio.h>
#include<conio.h>

main()
{

	
	int x =30;
	int y=40;
	
	
	printf("%d\n",(x<y));
	printf("%d\n",(x>y));
	printf("%d\n",(x<=y));
	printf("%d\n",(x>=y));
	printf("%d\n",(x==y));
	printf("%d\n",(x!=y));
	
	
	return 0;
	
}

